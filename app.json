{
  "expo": {
    "userInterfaceStyle": "automatic",
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "name": "droidconke-reactnative",
    "slug": "droidconke-reactnative",
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff",
        "dark": {
          "image": "./assets/images/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      },
      "package": "com.anonymous.ke.droidcon.reactnative"
    },
    "ios": {
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff",
        "dark": {
          "image": "./assets/images/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      }
    }
  }
}
