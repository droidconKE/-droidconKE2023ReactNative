{
  "expo": {
    "userInterfaceStyle": "automatic",
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "name": "DroidconKe ReactNative",
    "slug": "droidconke-reactnative",
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "icon": "./assets/images/icon.png",
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff",
        "dark": {
          "image": "./assets/images/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.ke.droidcon.reactnative.io",
              "pathPrefix": "/records"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "package": "com.ke.droidcon.reactnative"
    },
    "ios": {
      "icon": "./assets/images/icon.png",
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff",
        "dark": {
          "image": "./assets/images/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      },
      "bundleIdentifier": "com.ke.droidcon.reactnative"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6ad9531f-f082-4bfb-9213-272fdc94f01e"
      }
    },
    "owner": "reactdevske-reactnative",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/6ad9531f-f082-4bfb-9213-272fdc94f01e"
    }
  }
}
